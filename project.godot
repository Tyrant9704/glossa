; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Glossa"
run/main_scene="res://Scenes/menu.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
boot_splash/image="res://Resources/logo_noBGpng.png"
boot_splash/fullsize=false
config/icon="res://icon.svg"

[autoload]

Enums="*res://Scripts/enums.gd"
global="*res://Scripts/global.gd"
save_file="*res://Scripts/save_file.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=1280
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1

[dotnet]

project/assembly_name="WordleTutorial"

[internationalization]

locale/translations=PackedStringArray("res://Translations/translations .eng.translation", "res://Translations/translations .pol.translation", "res://Translations/translations .ukr.translation")

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
environment/defaults/default_clear_color=Color(0.101961, 0.101961, 0.105882, 1)
